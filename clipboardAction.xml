<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/clipboardAction.xsd">
	<import>
		<!-- pois -->
		<action name="enable">
			<actionclassname>poi\system\clipboard\action\PoiClipboardAction</actionclassname>
			<showorder>5</showorder>
			<pages>
				<page>poi\page\PoiListPage</page>
				<page>wcf\page\DeletedContentListPage</page>
			</pages>
		</action>
		<action name="disable">
			<actionclassname>poi\system\clipboard\action\PoiClipboardAction</actionclassname>
			<showorder>6</showorder>
			<pages>
				<page>poi\page\PoiListPage</page>
				<page>wcf\page\DeletedContentListPage</page>
			</pages>
		</action>
		<action name="delete">
			<actionclassname>poi\system\clipboard\action\PoiClipboardAction</actionclassname>
			<showorder>3</showorder>
			<pages>
				<page>poi\page\PoiListPage</page>
				<page>wcf\page\DeletedContentListPage</page>
			</pages>
		</action>
		<action name="trash">
			<actionclassname>poi\system\clipboard\action\PoiClipboardAction</actionclassname>
			<showorder>2</showorder>
			<pages>
				<page>poi\page\PoiListPage</page>
				<page>wcf\page\DeletedContentListPage</page>
			</pages>
		</action>
		<action name="restore">
			<actionclassname>poi\system\clipboard\action\PoiClipboardAction</actionclassname>
			<showorder>4</showorder>
			<pages>
				<page>poi\page\PoiListPage</page>
				<page>wcf\page\DeletedContentListPage</page>
			</pages>
		</action>
	</import>
</data>
